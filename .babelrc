{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"ie": "10",
					"edge": "17",
					"firefox": "60",
					"chrome": "67",
					"safari": "11.1"
				}
				// "useBuiltIns": "usage",
				// "corejs": 3
			}
		],
		"@babel/preset-react"
	],
	"plugins": [["@babel/plugin-transform-runtime", {
		"corejs": 3
    }], ["@babel/plugin-proposal-class-properties"],"@babel/plugin-proposal-object-rest-spread",
        ["import", {"libraryName": "antd", "libraryDirectory": "lib", "style":"css"
        }]]
}